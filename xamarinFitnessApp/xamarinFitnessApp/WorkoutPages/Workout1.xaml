<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="xamarinFitnessApp.WorkoutPages.Workout1">
    <ContentPage.Content>
        <StackLayout >
            <Label x:Name="lblWorkout" TextColor="White" Text="{Binding testMe}"></Label>
            <Label x:Name="lblw1" TextColor="White">first exercise</Label>
            <Label x:Name="lblw2" TextColor="White">second exercise</Label>
            <Label x:Name="lblw3" TextColor="White">third exercise</Label>
            <Label x:Name="lblw4" TextColor="White">fourth exercise</Label>
            <Button Text="Start!" Clicked="Button_Clicked"></Button>
       
        </StackLayout>
    </ContentPage.Content>
</ContentPage>