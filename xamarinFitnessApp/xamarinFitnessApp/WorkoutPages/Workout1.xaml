<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="xamarinFitnessApp.WorkoutPages.Workout1">
    <ContentPage.Content>
        
        <StackLayout >


            <StackLayout Padding="20">

                <Label Text="You will do:"  HorizontalOptions="Center" FontSize="Large" TextColor="White"></Label>

            </StackLayout>



            <Label x:Name="lblWorkout" TextColor="White" FontSize="Medium" Text="{Binding testMe}"></Label>
            <Label x:Name="lblw1" TextColor="White" FontSize="Medium">first exercise</Label>
            <Label x:Name="lblw2" TextColor="White" FontSize="Medium">second exercise</Label>
            <Label x:Name="lblw3" TextColor="White" FontSize="Medium">third exercise</Label>
            <Label x:Name="lblw4" TextColor="White" FontSize="Medium">fourth exercise</Label>
            <Label x:Name="lblw5" TextColor="White" FontSize="Medium"></Label>
            <Button Text="Start!" Clicked="Button_Clicked" StyleClass="Info" VerticalOptions="EndAndExpand" HorizontalOptions="Center"></Button>
       
        </StackLayout>
    </ContentPage.Content>
</ContentPage>